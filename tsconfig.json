{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": [
			"esnext",
			"dom"
		],
		// "declaration": true,
		// "declarationMap": true,
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": ".",
		"removeComments": true,
		"strict": true,
		"esModuleInterop": true,
		// "experimentalDecorators": true,
		// "emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}